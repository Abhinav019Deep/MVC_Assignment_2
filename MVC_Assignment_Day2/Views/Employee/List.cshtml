
@{
    ViewData["Title"] = "List";
}

<h1>List</h1>

<table>
    <tr>

        <th>Id</th>
        <th>Name</th>
        <th>Department</th>
        <th>Manager</th>
    </tr>
@{
    foreach(var emp in ViewBag.list)
    {
        <tr>
            <td>@emp.Id</td>
            <td>@emp.Name</td>
            <td>@emp.Dept</td>
            <td>@emp.Manager</td>
        </tr>
    }


}
</table>

